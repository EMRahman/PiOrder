proc            /proc           proc    defaults          0       0
/dev/mmcblk0p6  /boot           vfat    defaults,noatime  0       2
/dev/mmcblk0p7  /               ext4    defaults,noatime  0       1
tmpfs 		/tmp 		tmpfs   defaults,noatime,nodev,nosuid,size=100M  0 0 
tmpfs		/var/www/tmp	tmpfs	defaults,noatime,nodev,nosuid,size=10M  0 0 
tmpfs           /var/log        tmpfs   defaults,noatime,nosuid,mode=0755,size=100m 0 0
tmpfs           /var/tmp        tmpfs   defaults,noatime,nosuid,size=30m 0 0
tmpfs           /var/run        tmpfs   defaults,noatime,nosuid,mode=0755,size=5m 0 0
# a swapfile is not a swap partition, so no using swapon|off from here on, use  dphys-swapfile swap[on|off]  for that
